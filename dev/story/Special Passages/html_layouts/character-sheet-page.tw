<<script>>
(function waitForCharacterSheetReady(retries = 20) {
	const overlay = document.querySelector('.overlay-content');
	const charNameEl = document.getElementById('char-name');

	if (overlay && overlay.offsetParent !== null && charNameEl) {
		if (typeof updateCharacterSheet === 'function' && State?.variables?.player) {
			updateCharacterSheet(State.variables.player);
		}
	} else if (retries > 0) {
		setTimeout(() => waitForCharacterSheetReady(retries - 1), 100);
	} else {
		console.warn("[CharacterSheet] Gave up waiting for overlay to load.");
	}
})();
<</script>>





