:: CharacterSheetPage [nobr]
<!-- Modern Character Sheet -->
<div class="overlay-header">
    <h2>Character</h2>
    <button class="overlay-close-btn" onclick="cleanupCharacterSheet(); closeOverlay()">ร</button>
</div>

<!-- Tab Navigation -->
<div class="character-tabs">
    <button class="tab-button active" onclick="switchCharacterTab('status')">Status & Equipment</button>
    <button class="tab-button" onclick="switchCharacterTab('skills')">Attributes & Skills</button>
    <button class="tab-button" onclick="switchCharacterTab('combat')">Combat Skills</button>
    <button class="tab-button" onclick="switchCharacterTab('appearance')">Appearance</button>
</div>

<div class="overlay-content">
    <div class="character-sheet-container">
        <!-- Hero Section (Always Visible) -->
        <section class="hero-section">
            <div class="character-avatar">
                <img src="images/silhouette_placeholder.png" alt="Character Portrait" id="char-portrait">
                <div class="level-badge" id="char-level-badge">1</div>
            </div>
            
            <div class="character-info">
                <h1 id="char-name">Jaylie</h1>
                <div class="character-details">
                    <span class="detail-item"><span id="char-race">Human</span> <span id="char-gender">Female</span></span>
                    <span class="detail-item">Age: <span id="char-age">18</span></span>
                    <span class="detail-item">Born: <span id="char-dob">15th of Rain, 12219 AI</span></span>
                </div>
            </div>
            
            <div class="xp-container">
                <div class="xp-label">Experience Progress</div>
                <div class="xp-bar">
                    <div class="xp-fill" id="char-xp-fill" style="width: 0%"></div>
                    <div class="xp-text" id="char-xp-text">0 / 100</div>
                </div>
            </div>
        </section>

        <!-- Tab Content -->
        <div class="tab-content">
            <!-- Status Tab -->
            <div id="status-tab" class="tab-panel active">
                <div class="main-content">
                    <!-- Status Card -->
                    <section class="card">
                        <h2>Vitals & Status</h2>
                        <div class="status-grid">
                            <div class="status-item status-health">
                                <div class="status-header">
                                    <span class="status-name">Health</span>
                                    <span class="status-value" id="status-health-text">100/100</span>
                                </div>
                                <div class="status-bar">
                                    <div class="status-fill" id="status-health-fill" style="width: 100%"></div>
                                </div>
                            </div>
                            <div class="status-item status-fatigue">
                                <div class="status-header">
                                    <span class="status-name">Fatigue</span>
                                    <span class="status-value" id="status-fatigue-text">25/100</span>
                                </div>
                                <div class="status-bar">
                                    <div class="status-fill" id="status-fatigue-fill" style="width: 25%"></div>
                                </div>
                            </div>
                            <div class="status-item status-composure">
                                <div class="status-header">
                                    <span class="status-name">Composure</span>
                                    <span class="status-value" id="status-composure-text">100/100</span>
                                </div>
                                <div class="status-bar">
                                    <div class="status-fill" id="status-composure-fill" style="width: 100%"></div>
                                </div>
                            </div>
                            <div class="status-item status-excitement">
                                <div class="status-header">
                                    <span class="status-name">Excitement</span>
                                    <span class="status-value" id="status-excitement-text">0/100</span>
                                </div>
                                <div class="status-bar">
                                    <div class="status-fill" id="status-excitement-fill" style="width: 0%"></div>
                                </div>
                            </div>
                        </div>
                        <div class="conditions-list" id="char-conditions">
                            <!-- Dynamically populated conditions -->
                        </div>
                    </section>

                    <!-- Equipment Card -->
                    <section class="card">
                        <h2>Equipment</h2>
                        <div class="equipment-grid">
                            <div class="equipment-slot" data-slot="weaponMain">
                                <span class="slot-label">Main Hand</span>
                                <span class="slot-item" id="equip-weaponMain">Fists</span>
                            </div>
                            <div class="equipment-slot" data-slot="weaponOffhand">
                                <span class="slot-label">Off Hand</span>
                                <span class="slot-item" id="equip-weaponOffhand">None</span>
                            </div>
                            <div class="equipment-slot" data-slot="armorChest">
                                <span class="slot-label">Chest</span>
                                <span class="slot-item" id="equip-armorChest">Simple Clothes</span>
                            </div>
                            <div class="equipment-slot" data-slot="gloves">
                                <span class="slot-label">Gloves</span>
                                <span class="slot-item" id="equip-gloves">None</span>
                            </div>
                            <div class="equipment-slot" data-slot="armorLegs">
                                <span class="slot-label">Legs</span>
                                <span class="slot-item" id="equip-armorLegs">None</span>
                            </div>
                            <div class="equipment-slot" data-slot="armorShoes">
                                <span class="slot-label">Shoes</span>
                                <span class="slot-item" id="equip-armorShoes">None</span>
                            </div>
                            <div class="equipment-slot" data-slot="accessoryFace">
                                <span class="slot-label">Face</span>
                                <span class="slot-item" id="equip-accessoryFace">None</span>
                            </div>
                            <div class="equipment-slot" data-slot="accessoryNeck">
                                <span class="slot-label">Neck</span>
                                <span class="slot-item" id="equip-accessoryNeck">None</span>
