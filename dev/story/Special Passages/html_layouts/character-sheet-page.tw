                                <span class="slot-label">Back</span>
                                <span class="slot-item" id="equip-accessoryBack">None</span>
                            </div>
                            <div class="equipment-slot" data-slot="accessoryRing1">
                                <span class="slot-label">Ring 1</span>
                                <span class="slot-item" id="equip-accessoryRing1">None</span>
                            </div>
                            <div class="equipment-slot" data-slot="accessoryRing2">
                                <span class="slot-label">Ring 2</span>
                                <span class="slot-item" id="equip-accessoryRing2">None</span>
                            </div>
                            <div class="equipment-slot" data-slot="accessoryRing3">
                                <span class="slot-label">Ring 3</span>
                                <span class="slot-item" id="equip-accessoryRing3">None</span>
                            </div>
                        </div>
                    </section>

                    <!-- Carry Weight Card -->
                    <section class="card full-width">
                        <h2>Inventory Capacity</h2>
                        <div class="carry-weight-container">
                            <div class="carry-weight-bar">
                                <div class="carry-weight-fill" id="carry-weight-fill" style="width: 0%"></div>
